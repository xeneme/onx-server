lass=\"app-page-layout\" :style=\"computedStyle\">\n    <slot></slot>\n    <slot name=\"content\" />\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"app-page-layout\",\n  props: {\n    mobileWidth: {\n      type: Number,\n      default: 767\n    }\n  },\n  computed: {\n    computedStyle() {\n      return {backgroundColor: this.$themes.darkBackground}\n    }\n  },\n  mounted() {\n    window.addEventListener(\"resize\", () => this.updateSidebarState())\n\n    this.updateSidebarState()\n  },\n  beforeDestroy() {\n    window.removeEventListener(\"resize\", () => this.updateSidebarState())\n  },\n  methods: {\n    checkIsDesktop() {\n      return window.matchMedia(`(min-width: ${this.mobileWidth}px)`).matches\n    },\n    updateSidebarState() {\n      if (this.checkIsDesktop()) {\n        this.$emit(\"update:minimized\", false)\n      } else {\n        this.$emit(\"update:minimized\", true)\n      }\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\">\n.app-page-layout {\n  position: fixed;\n  top: 0;\n  right: 0;\n  left: 0;\n  bottom: 0;\n  height: 100vh;\n\n  .content-wrap {\n    transition: margin-left 0.3s ease;\n    padding: 0;\n    margin-left: 0 !important;\n  }\n}\n</style>\n"],"sourceRoot":"src/components/admin"}]}