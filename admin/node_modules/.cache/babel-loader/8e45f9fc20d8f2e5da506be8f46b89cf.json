AARA,GAvBA;AAiCA,EAAA,OAjCA,qBAiCA;AACA,SAAA,UAAA,GAAA,CAAA;AACA;AAnCA,CAAA","sourcesContent":["<template>\r\n  <va-inner-loading :loading=\"loading && log.length\">\r\n    <div v-if=\"log.length\" class=\"tab\" :style=\"computedStyle\">\r\n      <div class=\"row\">\r\n        <div class=\"flex xs12 xl12\">\r\n          <div\r\n            v-for=\"action in log[activePage - 1]\"\r\n            :key=\"action._id\"\r\n            class=\"tab__item d-flex align--center\"\r\n          >\r\n            <va-icon\r\n              :name=\"actionIcon(action.actionName)\"\r\n              class=\"ml-3 mr-3\"\r\n            ></va-icon>\r\n            <div>\r\n              {{ $t(action.messageLocalPath) }}\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"tab__pagination_wrap\" v-if=\"log.length > 1\">\r\n        <va-pagination\r\n          v-model=\"activePage\"\r\n          :pages=\"log.length\"\r\n          visiblePages=\"5\"\r\n          small\r\n        ></va-pagination>\r\n      </div>\r\n    </div>\r\n    <VaCardNoData v-else :loading=\"loading\" LT=\"Log is loading...\" ET=\"Empty\" />\r\n  </va-inner-loading>\r\n</template>\r\n\r\n<script>\r\nimport VaCardNoData from '../components/VaCardNoData'\r\nimport _ from 'lodash'\r\n\r\nexport default {\r\n  name: 'profile-log-tab',\r\n  components: {\r\n    VaCardNoData,\r\n  },\r\n  inject: ['user'],\r\n  data: () => ({\r\n    activePage: 1,\r\n  }),\r\n  props: {\r\n    loading: Boolean,\r\n  },\r\n  computed: {\r\n    computedStyle() {\r\n      return {\r\n        color: this.$themes.brightForeground,\r\n        opacity: this.loading ? 0.2 : 1,\r\n      }\r\n    },\r\n    log() {\r\n      return _.chunk(this.user.log, 6)\r\n    },\r\n  },\r\n  methods: {\r\n    actionIcon(name) {\r\n      return {\r\n        registered: 'fa fa-user-plus',\r\n        authenticated: 'fa fa-sign-in',\r\n        visited: 'fa fa-map-marker',\r\n        name_changed: 'fa fa-edit',\r\n      }[name]\r\n    },\r\n  },\r\n  mounted() {\r\n    this.activePage = 1\r\n  },\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.tab {\r\n  min-height: 232px;\r\n\r\n  &__wrap {\r\n    max-height: 400px;\r\n  }\r\n\r\n  &__item {\r\n    height: 55px;\r\n    width: calc(100% + 40px);\r\n    padding: 0 20px;\r\n    margin: 0 -20px;\r\n\r\n    &-name {\r\n      min-width: 80px;\r\n      max-width: 80px;\r\n    }\r\n\r\n    &:nth-child(odd) {\r\n      background-color: #ffffff07;\r\n    }\r\n  }\r\n\r\n  &__pagination_wrap {\r\n    display: grid;\r\n    justify-content: center;\r\n    height: 2rem;\r\n  }\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components/dashboard/dashboard-profile-tabs"}]}