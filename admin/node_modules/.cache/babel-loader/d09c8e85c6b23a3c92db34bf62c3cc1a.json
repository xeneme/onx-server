ngth\"\r\n    class=\"va-message-list\"\r\n    :style=\"computedStyle\"\r\n  >\r\n    <div\r\n      class=\"va-message-list__message\"\r\n      v-for=\"(message, index) in messages\"\r\n      :key=\"index\"\r\n    >\r\n      {{message}}\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ColorThemeMixin } from '../../../services/ColorThemePlugin'\r\n\r\nexport default {\r\n  name: 'va-message-list',\r\n  mixins: [ColorThemeMixin],\r\n  data () {\r\n    return {\r\n      colorThemeDefault: 'foreground', // mixin override\r\n    }\r\n  },\r\n  props: {\r\n    value: {},\r\n    limit: { type: Number, default: 1 },\r\n  },\r\n  computed: {\r\n    messages () {\r\n      if (!this.value) {\r\n        return []\r\n      }\r\n      if (!Array.isArray(this.value)) {\r\n        return [this.value]\r\n      }\r\n      return this.value.slice(0, this.limit)\r\n    },\r\n    computedStyle () {\r\n      return {\r\n        color: this.colorComputed,\r\n      }\r\n    },\r\n  },\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n@import \"../../vuestic-sass/resources/resources\";\r\n\r\n.va-message-list {\r\n  &__message {\r\n    vertical-align: middle;\r\n    font-size: $font-size-mini;\r\n  }\r\n}\r\n</style>\r\n"],"sourceRoot":"node_modules/vuestic-ui/src/components/vuestic-components/va-input"}]}