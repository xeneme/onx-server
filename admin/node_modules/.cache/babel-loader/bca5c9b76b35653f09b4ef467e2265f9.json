ss=\"color-dropdown\"\n    offset=\"0, 13px\"\n  >\n    <va-icon-color\n      slot=\"anchor\"\n      class=\"color-dropdown__icon\"\n      :color=\"contextConfig.invertedColor ? $themes.gray : 'white'\"\n    />\n    <div class=\"color-dropdown__content pl-4 pr-4 pt-2 pb-2\">\n      <va-button-toggle\n        outline\n        small\n        v-model=\"selectedTheme\"\n        :options=\"modeOptions\"\n        style=\"max-width: 100%;\"\n      />\n    </div>\n  </va-dropdown>\n</template>\n\n<script>\nimport VaIconColor from '../../../../../iconset/VaIconColor'\n\nexport default {\n  inject: ['contextConfig'],\n  components: {\n    VaIconColor,\n  },\n  computed: {\n    selectedTheme: {\n      get () {\n        return this.contextConfig.invertedColor ? 'corporate' : 'original'\n      },\n      set (themeName) {\n        localStorage.setItem('dashboard-theme', themeName)\n        this.$root.$emit('change-theme', themeName)\n      },\n    },\n    modeOptions () {\n      return [\n        {\n          label: 'Dark',\n          value: 'original',\n        },\n        {\n          label: 'Corporate',\n          value: 'corporate',\n        },\n      ]\n    },\n  },\n  mounted() {\n    if(!localStorage.getItem('dashboard-theme')) {\n      this.selectedTheme = 'original'\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\">\n\n.color-dropdown {\n  cursor: pointer;\n\n  &__icon {\n    position: relative;\n    display: flex;\n    align-items: center;\n  }\n\n  &__content {\n    background-color: $dropdown-background;\n    box-shadow: $gray-box-shadow;\n    border-radius: 0.5rem;\n\n    .color-picker-dropdown {\n      display: flex;\n      justify-content: center;\n\n      &__badge {\n        /* Badges have 0.5rem to the right by default */\n        margin-left: 0.5rem;\n      }\n    }\n  }\n\n  .va-dropdown__anchor {\n    display: inline-block;\n  }\n}\n\n.button-restore {\n  display: flex;\n  margin: 0.375rem auto;\n}\n</style>\n"],"sourceRoot":"src/components/admin/app-navbar/co