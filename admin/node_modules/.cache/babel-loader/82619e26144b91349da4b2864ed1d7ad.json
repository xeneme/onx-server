,CAAA;AACA,KAJA;AAMA,kBANA,yBAMA;AACA,WAAA,aAAA,GAAA,gBAAA,CAAA,KAAA,OAAA,CAAA;AACA,WAAA,cAAA,GAAA,iBAAA,CAAA,KAAA,OAAA,CAAA;AACA,KATA;AAWA,oBAXA,2BAWA;AACA,WAAA,cAAA,GAAA,iBAAA,CAAA,KAAA,OAAA,CAAA;AACA;AAbA,GAVA;AAyBA,EAAA,OAAA,EAAA;AACA,IAAA,aADA,2BACA;AACA,WAAA,wBAAA,IAAA,CAAA;AACA,WAAA,aAAA,GAAA,gBAAA,CAAA,KAAA,OAAA,EAAA,KAAA,wBAAA,CAAA;AACA,WAAA,KAAA,CAAA,SAAA,CAAA,KAAA,CAAA,KAAA,CAAA,OAAA;AACA,KALA;AAMA,IAAA,UANA,wBAMA;AACA,UAAA,GAAA,GAAA,MAAA,CAAA,IAAA,CAAA,EAAA,EAAA,OAAA,EAAA,sBAAA,CAAA;AACA,MAAA,GAAA,CAAA,QAAA,CAAA,KAAA,yBAAA,KAAA,iBAAA,UAFA,CAGA;;AACA,MAAA,UAAA,CAAA,YAAA;AACA,QAAA,GAAA,CAAA,QAAA,CAAA,KAAA;AACA,QAAA,GAAA,CAAA,KAAA;AACA,QAAA,GAAA,CAAA,KAAA;AACA,QAAA,GAAA,CAAA,KAAA;AACA,OALA,EAKA,GALA,CAAA;AAMA;AAhBA,GAzBA;AA2CA,EAAA,QAAA,EAAA;AACA,IAAA,iBADA,+BACA;AACA,aAAA,QAAA,CAAA,aAAA,CAAA,sBAAA,EAAA,SAAA,CAAA,WAAA,CAAA;AACA;AAHA;AA3CA,CAAA","sourcesContent":["<template>\n  <div class=\"row row-equal\">\n    <div class=\"flex xs12 xl6\">\n      <va-card :title=\"$t('dashboard.charts.trendyTrends')\">\n        <va-button\n          small\n          slot=\"actions\"\n          color=\"danger\"\n          class=\"mr-0\"\n          @click=\"deleteSection\"\n          :disabled=\"lineChartData.labels.length < 2\"\n        >\n          {{ $t('dashboard.charts.showInMoreDetail') }}\n        </va-button>\n        <va-chart class=\"chart\" ref=\"lineChart\" :data=\"lineChartData\" type=\"line\"/>\n      </va-card>\n    </div>\n\n    <div class=\"flex xs12 md6 xl3\">\n      <va-card :title=\"$t('dashboard.charts.loadingSpeed')\">\n        <va-button\n          icon=\"fa fa-print\"\n          flat\n          slot=\"actions\"\n          class=\"mr-0\"\n          @click=\"printChart\"\n        />\n        <va-chart class=\"chart chart--donut\" :data=\"donutChartData\" type=\"donut\"/>\n      </va-card>\n    </div>\n\n    <div class=\"flex xs12 md6 xl3\">\n      <dashboard-contributors-chart/>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { getDonutChartData } from '../../data/charts/DonutChartData'\nimport { getLineChartData } from '../../data/charts/LineChartData'\nimport DashboardContributorsChart from './DashboardContributorsList'\n\nexport default {\n  name: 'dashboard-charts',\n  components: { DashboardContributorsChart },\n  data () {\n    return {\n      lineChartData: getLineChartData(this.$themes),\n      donutChartData: getDonutChartData(this.$themes),\n      lineChartFirstMonthIndex: 0,\n    }\n  },\n  watch: {\n    '$themes.primary' () {\n      this.lineChartData = getLineChartData(this.$themes)\n      this.donutChartData = getDonutChartData(this.$themes)\n    },\n\n    '$themes.info' () {\n      this.lineChartData = getLineChartData(this.$themes)\n      this.donutChartData = getDonutChartData(this.$themes)\n    },\n\n    '$themes.danger' () {\n      this.donutChartData = getDonutChartData(this.$themes)\n    },\n  },\n  methods: {\n    deleteSection () {\n      this.lineChartFirstMonthIndex += 1\n      this.lineChartData = getLineChartData(this.$themes, this.lineChartFirstMonthIndex)\n      this.$refs.lineChart.$refs.chart.refresh()\n    },\n    printChart () {\n      const win = window.open('', 'Print', 'height=600,width=800')\n      win.document.write(`<br><img src='${this.donutChartDataURL}'/>`)\n      // TODO: find better solution how to remove timeout\n      setTimeout(() => {\n        win.document.close()\n        win.focus()\n        win.print()\n        win.close()\n      }, 200)\n    },\n  },\n  computed: {\n    donutChartDataURL () {\n      return document.querySelector('.chart--donut canvas').toDataURL('image/png')\n    },\n  },\n}\n</script>\n\n<style scoped>\n  .chart {\n    height: 400px;\n  }\n</style>\n"],"sourceRoot":"src/components/dashboard"}]}