n\n      class=\"app-topbar-link__icon\"\n      :name=\"iconRight\"\n      :style=\"computedIconStyles\"\n    />\n  </router-link>\n  <a\n    class=\"app-topbar-link\"\n    v-else\n    @mouseenter=\"updateHoverState(true)\"\n    @mouseleave=\"updateHoverState(false)\"\n    :style=\"computedStyle\"\n    :href=\"href\"\n  >\n    <va-icon\n      v-if=\"icon\"\n      class=\"app-topbar-link__icon\"\n      :style=\"computedIconStyles\"\n      :name=\"icon\"\n    />\n    <div class=\"app-topbar-link__title\">\n      <slot>\n        {{title}}\n      </slot>\n    </div>\n    <va-icon\n      class=\"app-topbar-link__icon\"\n      :name=\"iconRight\"\n      :style=\"computedIconStyles\"\n    />\n  </a>\n</template>\n\n<script>\nimport { colorShiftHsl, ColorThemeMixin } from './../../../../services/vuestic-ui'\n\nexport default {\n  name: 'topbar-link',\n  inject: ['contextConfig'],\n  mixins: [ColorThemeMixin],\n  props: {\n    to: {\n      type: [Object, String],\n      default: '',\n    },\n    target: {\n      type: String,\n      default: '_self',\n    },\n    icon: {\n      type: [String, Array],\n    },\n    iconRight: {\n      type: [String, Array],\n    },\n    title: {\n      type: String,\n    },\n    isActive: {\n      type: Boolean,\n      default: false,\n    },\n    href: {\n      type: String,\n    },\n  },\n  data () {\n    return {\n      isHovered: false,\n    }\n  },\n  computed: {\n    computedStyle () {\n      if (this.contextConfig.invertedColor) {\n        if (this.isHovered || this.isActive) {\n          return {\n            color: this.$themes.primary,\n            borderColor: colorShiftHsl(this.$themes.primary, { s: 13, l: -15 }).css,\n          }\n        }\n\n        return {\n          color: this.$themes.gray,\n        }\n      }\n\n      if (this.isActive || this.isHovered) {\n        return {\n          color: this.$themes.brightForeground,\n          borderColor: this.$themes.primary,\n          backgroundColor: this.$themes.darkestBackground,\n        }\n      }\n\n      return {\n        color: this.$themes.foreground,\n      }\n    },\n    computedIconStyles () {\n      if (this.contextConfig.invertedColor) {\n        if (this.isHovered || this.isActive) {\n          return {\n            color: this.$themes.primary,\n          }\n        }\n\n        return {\n          color: this.$themes.gray,\n        }\n      }\n\n      if (this.isHovered || this.isActive) {\n        return {\n          color: this.$themes.primary,\n        }\n      }\n\n      return {\n        color: this.$themes.foreground,\n      }\n    },\n  },\n  methods: {\n    updateHoverState (isHovered) {\n      this.isHovered = isHovered\n    },\n    updateActiveState () {\n      this.isActive = (this.$route.name === this.to.name) || this.activeByDefault\n    },\n    mounted () {\n      this.updateActiveState()\n    },\n  },\n}\n</script>\n\n<style lang=\"scss\">\n\n.app-topbar-link {\n  position: relative;\n  cursor: pointer;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  text-decoration: none;\n  border-left: none;\n  border-bottom: 0.25rem solid transparent;\n  height: 4rem;\n  padding: 1.25rem 0.25rem 1rem;\n\n  @include media-breakpoint-down(sm) {\n    flex-grow: 1;\n  }\n\n  &__icon {\n    width: 1.5rem;\n    text-align: center;\n    font-size: $sidebar-menu-item-icon-size;\n  }\n\n  &__title {\n    line-height: 1.5em;\n    margin-left: 0.5rem;\n\n    @include media-breakpoint-down(sm) {\n      display: none;\n    }\n  }\n}\n</style>\n"],"sourceRoot":"src/components/admin/app-topbar/components"}]}