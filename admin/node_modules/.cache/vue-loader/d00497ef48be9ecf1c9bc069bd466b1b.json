urn parent.color\r\n          },\r\n        },\r\n      }),\r\n    }\r\n  },\r\n  methods: {\r\n    /**\r\n     * @public\r\n     */\r\n    collapse () {\r\n      this.$nextTick(() => {\r\n        this.$children.forEach(child => {\r\n          if (child.$options.name === 'va-tree-category') {\r\n            child.collapse()\r\n          }\r\n        })\r\n      })\r\n    },\r\n    /**\r\n     * @public\r\n     */\r\n    expand () {\r\n      this.$nextTick(() => {\r\n        this.$children.forEach(child => {\r\n          if (child.$options.name === 'va-tree-category') {\r\n            child.expand()\r\n          }\r\n        })\r\n      })\r\n    },\r\n  },\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n.va-tree-root {\r\n  padding: .3125rem;\r\n}\r\n</style>\r\n"]}]}