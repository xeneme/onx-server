    {{ info }}\r\n            </div>\r\n            <div v-if=\"typeof profile[info] != 'object'\">\r\n              {{ profile[info] }}\r\n            </div>\r\n            <div v-else>\r\n              <span v-for=\"(el, i) in profile[info]\" :key=\"i\">\r\n                <a :href=\"el.url\" target=\"_blank\">\r\n                  {{ el.amount + ' ' + el.net }}\r\n                </a>\r\n                {{ i < profile[info].length - 1 ? ' | ' : '' }}\r\n              </span>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </va-inner-loading>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'profile-info-tab',\r\n  inject: ['user'],\r\n  props: {\r\n    loading: Boolean,\r\n  },\r\n  computed: {\r\n    computedStyle() {\r\n      return {\r\n        color: this.$themes.brightForeground,\r\n        opacity: this.loading ? 0.2 : 1,\r\n      }\r\n    },\r\n    profile() {\r\n      let p = {\r\n        ID: this.user.id,\r\n        Role: this.user.role.toUpperCase(),\r\n        Username: this.user.username,\r\n        'E-mail': this.user.email,\r\n        Transfers: this.user.transfers,\r\n      }\r\n\r\n      if (!this.user.me) p['Binded to'] = this.user.bindedTo || 'nobody'\r\n      if (!this.user.me && this.user.location) p['Location'] = `${this.user.location.country}, ${this.user.location.city} (${this.user.location.ip})`\r\n\r\n      return p\r\n    },\r\n  },\r\n  methods: {\r\n    infoIcon(name) {\r\n      return {\r\n        ID: 'fa fa-id-card-o',\r\n        Role: 'fa fa-user-o',\r\n        Username: 'fa fa-address-book',\r\n        'E-mail': 'fa fa-envelope-o',\r\n        Transfers: 'fa fa-money',\r\n        'Binded to': 'fa fa-link',\r\n        'Location': 'fa fa-map-marker',\r\n      }[name]\r\n    },\r\n  },\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.tab {\r\n  a {\r\n    color: #55a1fb;\r\n  }\r\n\r\n  a:hover {\r\n    text-decoration: underline;\r\n  }\r\n\r\n  &__item {\r\n    min-height: 55px;\r\n    width: calc(100% + 40px);\r\n    padding: 15px 20px;\r\n    margin: 0 -20px;\r\n\r\n    &-name {\r\n      min-width: 80px;\r\n      max-width: 80px;\r\n    }\r\n\r\n    &:nth-child(odd) {\r\n      background-color: #4d4d4d11;\r\n    }\r\n  }\r\n}\r\n</style>\r\n"]}]}