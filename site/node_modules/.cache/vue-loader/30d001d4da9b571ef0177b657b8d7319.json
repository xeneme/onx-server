ort { mapGetters } from 'vuex';\n\nexport default {\n  name: 'Exchange',\n  components: {\n    AlertsContainer,\n    Header,\n    Footer,\n    Tabs,\n    Wallet,\n    Deposit,\n    Withdraw,\n    Transfer,\n    History,\n    Background,\n  },\n  computed: {\n    ...mapGetters({\n      selectedTab: 'popups/exchangeCurrentTab',\n    }),\n  },\n  mounted() {\n    if (this.$route.hash === '#wallet') {\n      this.$store.commit('popups/SELECT_EXCHANGE_TAB', 0);\n    } else if (this.$route.hash === '#deposit') {\n      this.$store.commit('popups/SELECT_EXCHANGE_TAB', 1);\n    } else if (this.$route.hash === '#withdraw') {\n      this.$store.commit('popups/SELECT_EXCHANGE_TAB', 2);\n    } else if (this.$route.hash === '#transfer') {\n      this.$store.commit('popups/SELECT_EXCHANGE_TAB', 3);\n    } else if (this.$route.hash === '#history') {\n      this.$store.commit('popups/SELECT_EXCHANGE_TAB', 4);\n    }\n  },\n};\n</script>\n\n<style lang=\"sass\" scoped>\n@import \"../scss/smart-grid\"\n@import \"../scss/variables\"\n\n.page\n  background: $v-grad\n  background-size: contain\n\n  .container\n    display: flex\n    z-index: 1\n    min-height: 100vh\n    grid-template-rows: minmax(170px, 20vh) 4fr 200px\n    grid-gap: 20px\n    justify-content: center\n\n    @include xs-block\n      padding: 0\n      padding-left: 6px\n    @include sm-block\n      padding: 0\n      padding-left: 15px\n    @include md-block\n      padding: 0 40px\n    @include lg-block\n      padding: 0 40px\n</style>\n"]}]}