onents: { Icon },\n  props: {\n    title: {\n      type: String,\n      default: 'Bitcoin',\n    },\n    shortTitle: {\n      type: String,\n      default: 'BTH',\n    },\n    rate: {\n      type: String,\n      default: '0',\n    },\n    ratePercent: {\n      type: String,\n      default: '0',\n    },\n    link: {\n      type: String,\n      default: '#',\n    },\n    icon: {\n      type: String,\n      default: 'bitcoin',\n    },\n  },\n};\n</script>\n\n<style lang=\"sass\" scoped>\n@import \"../../../../scss/smart-grid\"\n@import \"../../../../scss/variables\"\n\n.currency\n  display: grid\n  grid-gap: 10px\n  grid-template-columns: 1fr 5fr 4fr 4fr 1fr 1fr\n  grid-template-rows: auto\n  grid-template-areas: \"icon title subtitle rate rate-percent charts\"\n\n  color: #fff\n  padding-bottom: 18px\n  font-weight: bold\n  width: 100%\n\n  @include xs-block\n    grid-template-columns: 4fr 1fr 1fr\n    grid-template-areas: \"title . subtitle\" \"rate rate-percent charts\"\n    border-bottom: 1px rgba(0, 50, 120, 1) solid\n  @include sm-block\n    grid-template-columns: 4fr 1fr 1fr\n    grid-template-areas: \"title . subtitle\" \"rate rate-percent charts\"\n    border-bottom: 1px rgba(0, 50, 120, 1) solid\n  @include md-block\n    grid-template-columns: 1fr 5fr 4fr 4fr 1fr 1fr\n    grid-template-areas: \"title subtitle rate rate-percent charts\"\n    border-bottom: none\n  @include lg-block\n    grid-template-columns: 1fr 5fr 4fr 4fr 1fr 1fr\n    grid-template-areas: \"icon title subtitle rate rate-percent charts\"\n    border-bottom: none\n\n  &__icon, &__title, &__subtitle, &__rate, &__rate-percent, &__link\n    place-self: center stretch\n\n  &__title, &__subtitle, &__rate-percent\n    color: $light-blue\n\n  &__icon\n    grid-column: 1\n    grid-area: icon\n    @include xs(display, none)\n    @include sm(display, none)\n    @include md(display, none)\n    @include lg(display, flex)\n\n  &:last-child\n    margin-bottom: 0\n\n  &__title\n    grid-column: 2\n    grid-area: title\n    text-align: left\n\n  &__subtitle\n    grid-column: 4\n    grid-area: subtitle\n    opacity: .5\n\n  &__title, &__subtitle\n    @include xs(font-size, 20px)\n    @include sm(font-size, 25px)\n    @include md(font-size, 15px)\n    @include lg(font-size, 18px)\n\n  &__rate\n    grid-column: 5\n    grid-area: rate\n\n    @include xs-block\n      text-align: left\n      color: #00FF88\n    @include sm-block\n      text-align: left\n      color: #00FF88\n    @include md-block\n      text-align: center\n      color: $light-blue\n    @include lg-block\n      text-align: center\n      color: $light-blue\n\n  &__subtitle, &__rate\n    text-transform: uppercase\n\n  &__rate-percent\n    grid-column: 6\n    grid-area: rate-percent\n    width: 100px\n\n  &__link\n    grid-column: 7\n    grid-area: charts\n\n  &__link:hover\n    opacity: .7\n</style>\n"]}]}