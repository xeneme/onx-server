  <Footer />\n  </div>\n</template>\n\n<script>\nimport Header from '@/components/header/Header.vue';\nimport Button from '@/components/partial/Button.vue';\nimport Currencies from '@/components/views/main/currencies/Currencies.vue';\nimport Advantages from '@/components/views/main/advantages/Advantages.vue';\nimport Rates from '@/components/views/main/rates/Rates.vue';\nimport Partners from '@/components/views/main/partners/Partners.vue';\nimport About from '@/components/views/main/about/About.vue';\nimport Footer from '@/components/footer/Footer.vue';\n\nexport default {\n  name: 'Main',\n  components: {\n    Header,\n    Button,\n    Currencies,\n    Advantages,\n    Rates,\n    Partners,\n    About,\n    Footer,\n  },\n  methods: {\n    showSignIn() {\n      this.$store.commit('popups/SIGN_IN', true);\n    },\n    showSignUp() {\n      this.$store.commit('popups/SIGN_UP', true);\n    },\n    sendViber() {\n      let response = '';\n\n      fetch('https://chatapi.viber.com/pa/send_message', {\n        method: 'POST',\n        mode: 'cors',\n        cache: 'no-cache',\n        credentials: 'same-origin',\n        headers: {\n          'Content-Type': 'application/json',\n        },\n        redirect: 'follow',\n        referrer: 'no-referrer',\n        body: JSON.stringify({\n          receiver: '+77476366956',\n        }),\n      }).then((response = response.json()));\n      console.log(response);\n    },\n  },\n};\n</script>\n\n<style lang=\"sass\" scoped>\n@import \"../scss/smart-grid\"\n@import \"../scss/variables\"\n\n.welcome\n  position: relative\n  overflow: hidden\n\n  &__container\n    position: relative\n    @include xs-block\n      height: auto\n      padding-bottom: 50px\n    @include sm-block\n      height: auto\n      padding-bottom: 350px\n    @include md-block\n      height: 102vh\n      padding-bottom: 0px\n    @include lg-block\n      height: 102vh\n      padding-bottom: 0px\n\n    z-index: 1\n\n    &__group-button\n      display: flex\n      flex-wrap: wrap\n      justify-content: center\n\n      button, a\n        @include to(110rem)\n          margin: 2rem 1rem 0 1rem\n        @include from(110rem)\n          margin: 3vh 1vh 0 1vh\n\n    &__title, &__subtitle\n      color: #fff\n      padding-left: 3rem\n      padding-right: 3rem\n\n    &__title\n      font-weight: bold\n      -webkit-text-stroke: 1px $cyan\n      text-shadow: $cyan 2px 2px\n\n      @include to(110rem)\n        margin-top: 20vh\n        margin-bottom: 2vh\n        font-size: 2.8rem\n      @include from(110rem)\n        font-size: 6vh\n        margin-top: 20vh\n        margin-bottom: 2vh\n\n    &__subtitle\n      margin: 0 auto\n\n      @include to(110rem)\n        font-size: 1rem\n        max-width: 250px\n      @include from(110rem)\n        font-size: 2vh\n        max-width: 50vh\n\n  .bg\n    position: relative\n    overflow: hidden\n\n    &__fullscreen-video\n      @include xs-block\n        left: -600px\n      @include sm-block\n        left: -400px\n      @include md-block\n        left: -150px\n      @include lg-block\n        left: 0\n\n      position: absolute\n      z-index: 0\n      object-fit: cover\n      top: 0\n      min-width: 100%\n      min-height: 100%\n      opacity: .6\n</style>\n"],"sourceRoot":"src/views"}]}