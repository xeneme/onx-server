class=\"profile__password\">\r\n          <p>•••••••••••••••••••••</p>\r\n        </div>\r\n      </div>\r\n      <div class=\"payment_method\">\r\n        <div class=\"payment_method__title\">\r\n          <p>Two factor authentication</p>\r\n          <a href=\"#change-payment\">set up</a>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <Footer />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapGetters } from 'vuex';\r\nimport Header from '@/components/header/Header.vue';\r\nimport Background from '@/components/views/exchange/Background';\r\nimport AlertsContainer from '@/components/partial/AlertsContainer';\r\nimport Footer from '@/components/footer/Footer';\r\nimport ChangeNameForm from '@/components/views/profile/ChangeNameForm';\r\nimport ChangePasswordForm from '@/components/views/profile/ChangePasswordForm';\r\n\r\nexport default {\r\n  name: 'Profile',\r\n  components: {\r\n    AlertsContainer,\r\n    Background,\r\n    Footer,\r\n    Header,\r\n    ChangeNameForm,\r\n    ChangePasswordForm,\r\n  },\r\n  computed: {\r\n    ...mapGetters({\r\n      changeNameFormVisibility: 'popups/changeNameVisibility',\r\n      changePasswordFormVisibility: 'popups/changePasswordVisibility',\r\n      profile: 'auth/profile',\r\n    }),\r\n    name() {\r\n      return (\r\n        this.profile.firstName.toLowerCase().capitalize() +\r\n        ' ' +\r\n        this.profile.lastName.toLowerCase().capitalize()\r\n      );\r\n    },\r\n  },\r\n  methods: {\r\n    showChangeNameForm() {\r\n      this.$store.commit('popups/CHANGE_NAME', true);\r\n    },\r\n    showChangePasswordForm() {\r\n      this.$store.commit('popups/CHANGE_PASSWORD', true);\r\n    },\r\n  },\r\n  mounted() {\r\n    if (!this.profile) {\r\n      this.$router.push('/');\r\n      return\r\n    }\r\n\r\n    this.$store.commit('popups/CHANGE_NAME', false);\r\n    this.$store.commit('popups/CHANGE_PASSWORD', false);\r\n\r\n    if (this.$route.hash === '#edit') {\r\n      this.$store.commit('popups/CHANGE_NAME', true);\r\n    } else if (this.$route.hash === '#change-password') {\r\n      this.$store.commit('popups/CHANGE_PASSWORD', true);\r\n    } else if (this.$route.hash === '#change-payment') {\r\n      this.$store.commit('popups/CHANGE_PAYMENT', true);\r\n    }\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"sass\" scoped>\r\n@import '../scss/smart-grid'\r\n@import '../scss/variables'\r\n\r\n.page\r\n  background: $v-grad\r\n  background-size: contain\r\n\r\n  .container\r\n    z-index: 1\r\n    margin-bottom: 200px\r\n\r\n.profile, .payment_method\r\n    margin: 0 auto\r\n    padding: 15px\r\n    backdrop-filter: blur(2px)\r\n    background: $cyan-alpha\r\n    opacity: .8\r\n    border-radius: 5px\r\n    display: grid\r\n    border: 1px solid #56A4FF77\r\n    grid-area: profile\r\n    color: white\r\n\r\n    @include xs-block\r\n        width: 90%\r\n    @include sm-block\r\n        width: 90%\r\n    @include md-block\r\n        width: 70%\r\n    @include lg-block\r\n        width: 50%\r\n\r\n    &__names\r\n        border-bottom: 1px solid $cyan-alpha\r\n\r\n    &__password\r\n        p\r\n            font-size: 2rem\r\n            line-height: 3rem\r\n            text-align: left\r\n\r\n        &__title\r\n            margin-top: 15px\r\n\r\n    &__names, &__password\r\n        width: 100%\r\n\r\n        &__title\r\n            width: 100%\r\n            display: flex\r\n            justify-content: space-between\r\n            font-family: Montserrat\r\n\r\n            a\r\n                color: $light-blue\r\n                text-decoration: none\r\n                font-weight: 600\r\n                text-transform: uppercase\r\n                font-size: 12px\r\n\r\n                &:hover\r\n                    color: white\r\n\r\n            p\r\n                color: white\r\n\r\n\r\n        &__profile\r\n            display: grid\r\n            width: 100%\r\n            grid-template-columns: 100px 1fr\r\n            justify-items: center\r\n            align-items: center\r\n            height: 80%\r\n\r\n            &_user\r\n                border-radius: 100%\r\n                width: 80px\r\n                height: 80px\r\n                background-color: $cyan\r\n                overflow: hidden\r\n                justify-self: end\r\n                display: grid\r\n                margin: 40px 0\r\n\r\n                svg\r\n                    width: 50%\r\n                    height: 50%\r\n                    place-self: center\r\n\r\n            &_name\r\n                margin-left: 20px\r\n                justify-self: start\r\n\r\n                p\r\n                    text-align: left\r\n\r\n                    &:first-child\r\n                        font-size: 1.6rem\r\n                        line-height: 2.1rem\r\n\r\n.payment_method\r\n    margin-top: 20px\r\n\r\n    &__title\r\n        width: 100%\r\n        display: flex\r\n        justify-content: space-between\r\n        font-family: Montserrat\r\n\r\n        a\r\n            color: $light-blue\r\n            text-decoration: none\r\n            font-weight: 600\r\n            text-transform: uppercase\r\n            font-size: 12px\r\n\r\n            &:hover\r\n                color: white\r\n\r\n        p\r\n            color: white\r\n</style>\r\n"],"sourceRoot":"src/views"}]}