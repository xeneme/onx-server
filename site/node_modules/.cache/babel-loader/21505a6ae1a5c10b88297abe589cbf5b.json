   border-radius: 10px 0 0 0\r\n\r\n    .profileinput_input\r\n        border-radius: 0 10px 0 0\r\n\r\n.bottom\r\n    .profileinput_icon_wrap\r\n        border-radius: 0 0 0 10px\r\n\r\n    .profileinput_input\r\n        border-radius: 0 0 10px 0\r\n\r\n.profileinput\r\n    z-index: 2\r\n    display: grid\r\n    grid-template-columns: 48px 1fr\r\n    grid-template-rows: 48px\r\n    margin: 0 auto\r\n    justify-content: end\r\n    overflow: hidden\r\n    margin-bottom: 40px\r\n\r\n    @include xs-block\r\n      width: 80%\r\n    @include sm-block\r\n      width: 405px\r\n\r\n    &_icon_wrap\r\n        width: calc(100% - 2px)\r\n        height: calc(100% - 2px)\r\n        border: 1px #0075ff22 solid\r\n        display: grid\r\n        background-color: #0075ff22\r\n\r\n    &_icon\r\n        display: grid\r\n        width: 40%\r\n        height: 40%\r\n        place-self: center\r\n        color: #0075ff88\r\n\r\n        *\r\n            place-self: center\r\n\r\n    &_input\r\n        color: #0094ff\r\n        text-indent: 21px\r\n        line-height: 45px\r\n        width: 99.7%\r\n        font-size: 1.1rem\r\n        letter-spacing: 0.04rem\r\n        background: darken($blue, 3)\r\n        transition: background-color 0.3s\r\n        border: none\r\n        box-shadow: 10px 0 20px #00002344 inset\r\n\r\n        &:focus\r\n            background: darken($blue, 7)\r\n            outline: none\r\n\r\n        &::selection\r\n            background: $cyan-alpha\r\n            color: white\r\n\r\n        &::placeholder\r\n            transition: all .3s\r\n            text-align: left\r\n            color: #0094ff55\r\n</style>\r\n"],"sourceRoot":"src/components/views/profile"}]}