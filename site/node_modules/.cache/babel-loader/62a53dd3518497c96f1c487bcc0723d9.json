ort('../views/Main.vue'),\n  },\n  {\n    path: '/exchange',\n    name: 'Exchange',\n    component: () => import('../views/Exchange.vue'),\n  },\n  {\n    path: '/debug',\n    name: 'Debug',\n    component: () => import('../views/Test.vue'),\n  },\n  {\n    path: '/account',\n    name: 'Profile',\n    component: () => import('../views/Profile.vue'),\n  },\n  {\n    path: '*',\n    name: '404',\n    component: () => import('../views/NotFound.vue'),\n  },\n];\n\n/* eslint-disable */\nconst router = new VueRouter({\n  mode: 'history',\n  base: process.env.BASE_URL,\n  routes,\n});\n\nrouter.beforeEach((to, from, next) => {\n  axios\n    .get(window.location.protocol + '//' + window.location.host + '/api/auth', {\n      headers: {\n        Authorization: sessionStorage.getItem('auth-token'),\n      },\n    })\n    .then(response => {\n      store.commit('auth/SET_PROFILE', {\n        token: response.data.token,\n        profile: response.data.profile,\n      });\n\n      if (to.name != from.name)\n        store.dispatch('preloader/startBeforeLoading').then(next);\n    })\n    .catch(() => {\n      if (['/exchange', '/account'].includes(to.path)) {\n        router.push('/');\n        store.commit('popups/SIGN_IN', true);\n      }\n\n      sessionStorage.removeItem('auth-token');\n\n      if (to.name != from.name)\n        store.dispatch('preloader/startBeforeLoading').then(next);\n    });\n});\n\nrouter.afterEach((to, from, next) => {\n  document.title = to.name != 'Main' ? 'Onyxian - ' + to.name : 'Onyxian';\n\n  store.dispatch('preloader/startAfterLoading').then(next);\n});\n\nexport default router;\n"]}]}